<?xml version='1.0' encoding='utf-8'?>
<widget id="com.supportbee.hackinat" version="0.0.10" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <preference name="AndroidLaunchMode" value="singleTop" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <feature name="PushPlugin">
        <param name="android-package" value="com.plugin.gcm.PushPlugin" />
    </feature>
    <icon density="mdpi" src="www/res/android/mdpi.png" />
    <icon density="hdpi" src="www/res/android/hdpi.png" />
    <icon density="xhdpi" src="www/res/android/xhdpi.png" />
    <icon density="xxhdpi" src="www/res/android/xxhdpi.png" />
    <icon density="xxxhdpi" src="www/res/android/xxxhdpi.png" />
    <name>Hackin.at</name>
    <description>
        An app that helps programmers meetup and socialize with other programmers
    </description>
    <author email="dev@supportbee.com" href="http://hackin.at">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="Geolocation">
        <param name="android-package" value="org.apache.cordova.GeoBroker" />
    </feature>
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
</widget>
